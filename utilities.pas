  MM_AREA: TBox := [570,12,714,156];//[570,9,714,159];
  MM_CROP: TBox := [0,0,MM_AREA.X2-MM_AREA.X1,MM_AREA.Y2-MM_AREA.Y1];
  MM_RAD: Int32 := 64;
  
