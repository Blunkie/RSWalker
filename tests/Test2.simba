program new;
{$I SRL/OSR.simba}
{$I OSRWalker/Walker.simba}

var
  finder:TRSPosFinder;
  loc:TPoint;
  t:UInt32;
begin
  finder.Init(w_getClientPID());
  finder.UpdateMap(True);
  AddOnTerminate(@finder.Free);

  while 1 do
  begin
    t := GetTimeRunning();
    loc := finder.GetLocalPos(True);

    ClearDebug();
    WriteLn(loc,' used: ', Round(GetTimeRunning() - t,3),'ms' );
    finder.DebugPos(loc);
  end;
end;
