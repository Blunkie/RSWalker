program new;
{$I SRL/OSR.simba}
{$I RSWalker/Walker.simba}

var
  RSW:TRSWalker;
  path:TPointArray;

procedure onMove(sender: PRSWalker);
begin
  RSW.Finder.DebugPos(RSW.Finder.GetLocalPos);
end;

begin
  RSW.InitEx('', 'world', [[3,5],[3,6],[3,7],[4,5],[4,6],[4,7],[5,5],[5,6],[5,7]]);
  RSW.MemScanEnabled := True;
  RSW.SkipClose := 20;
  RSW.onMoveEvent := @onMove;
  AddOnTerminate(@RSW.Free);

  WriteLn RSW.WebWalk(locVarrockWBank);
end.
